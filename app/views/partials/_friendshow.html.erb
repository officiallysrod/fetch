<div class="bio col-sm-4">
  <div ng-show="friends.length >= 1">
    <div class="bio-pic-wrapper">
    	<div bg-image="{{friend.profile_pic_large}}" class="bio-pic"></div>
    </div>
    <h3 class="bio-name">{{friend.dog_name}} &amp; {{friend.fname}}</h3>
    <p class="bio-description">{{friend.bio}}</p>
    </div>
  <div ng-show="friends.length < 1">
    <h3 class="bio-name">You don't have any matches yet!</h3>
  </div>
</div>