<!-- Show Modal -->
<div class="modal fade profileModal" id="{{user.id}}" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-body clearfix">
        <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
        <div class="people pull-left">
          <div class="picture" bg-image="{{user.profile_pic_medium}}"></div>
        </div>
        <div class="bio">
          <h3 class="bio-name">{{user.dog_name}} &amp; {{ user.fname }}</h3>
          <p class="bio-description">{{ user.bio }}</p>
        </div>
      </div>
    </div>
  </div>
</div>