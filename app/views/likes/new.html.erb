<div>
  <% @users.each do |user| %>
    <%= user.fname %> <%= user.lname %>
    <%= link_to "Like!", likes_path, method: :post %>
    <br>
  <% end %>
</div>

<div>
  
  <% @users.each do |user| %>
    <%= form_for @like do |f| %>
      <%= user.fname %> <%= user.lname %>
      <%= f.submit "Like!", class: "btn btn-primary btn-xs" %>
      <br>
    <% end %>
  <% end %>
</div>
